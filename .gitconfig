[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  sub = submodule
  lg = log -p
  lol = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all

[user]
  name = Dave Stillman
  email = david.stillman@sparkfun.com

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto

[push]
  default = current
